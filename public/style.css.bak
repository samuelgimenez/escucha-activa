html, body {
	font-family: Helvetica, Arial, sans-serif;
	margin: 0px;
	padding: 0px;
	font-size: 12px;
	text-align:center;
/*	background: url(./public/i/fondo.jpg) repeat-x #FFF;*/
	/*background-color: #eef2f4;*/
	background-color: #FFFFFF;
	line-height: 20px;	
	letter-spacing: 1px; 
}

div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, p, blockquote, table, th, td { margin: 0; padding: 0; }
img{
	border: 0;
	max-width: 100%;
} 
a { 
	text-align:left;
	font-size: 12px;
	color:#3b515c !important;
	text-decoration: none;
	outline:0;
	letter-spacing: 1px;
	font-weight: bold;
}

a:hover { 
	text-align:left;
	font-size: 12px;
	color:#000000 !important;
	text-decoration: none;
	letter-spacing: 1px;
	font-weight: bold;
}

.oculto {
	display: none;
	visibility: hidden;
}

table {
    border-collapse: separate;
}
ul { list-style:none; }
address { font-style: normal; }

ul.styleDiscUL { list-style: disc; }

.hueco30 { margin-top: 30px; }
.hueco60 { margin-top: 60px; }
.hueco60y30 { margin-top: 60px; margin-right: 30px; }

/* colores de la web */

.negroCalido { color: #332f21; }
.grisOscuro { color: #666359; }
.grisMedio { color: #999790; }
.grisClaro { color: #e5e5e3; }
.azulOscuro {color: #3b515c; }
.azul {color: #76a1b8; }
.negrita { font-weight: bold; }

/* fin colores de la web */

/* cabecera relacionados*/
#cabeceraRelacionados { width: 100%; background-color: #666359; float:right; /*height:20px;*/ overflow: hidden; }
/*#cabeceraRelacionados ul { float:right; letter-spacing:1px; }  
#cabeceraRelacionados li {float:left; padding-right: 18px; position:relative; right:50%; }RESPONSIVE */
#cabeceraRelacionados ul { float:left;position:relative; left:50%; letter-spacing:1px; line-height: 18px; width:980px; }
#cabeceraRelacionados li {float:right; padding-left: 18px; position:relative; right:50%; }

#cabeceraRelacionados li a{text-decoration:none; color:#e5e5e3 !important; font-size: 10px; }
#cabeceraRelacionados li.active a{color:#ffffff !important; font-weight:bold; border-bottom: 3px solid #76a1b8;}
#cabeceraRelacionados li a:hover{color:#ffffff !important;}

/* fin cabecera relacionados*/

/* cabecera institucional */
#cabecera { width: 100%; background-color: #FFFFFF; height: 80px; float: left; overflow: hidden; }
#cabecera ul { float:left; position:relative; left:50%;  width: 980px; }
#cabecera ul li { float: left; position:relative; right:50%; padding-top:15px; padding-left: 0px; padding-bottom: 9px; }

/* fin cabecera  institucional */

/* cabecera  aod*/
#banner {  width: 100%; background-color: red;  float: left; overflow: hidden; }
#banner ul { float:left; position:relative; left:50%;  width: 980px; }
#banner ul li { float: left; position:relative; right:50%; }

#banner ul li.busqueda { padding-left: 0px; padding-top: 0px; width:500px; }
#banner ul li.bannerBuscador { float: right; padding-top: 52px; }
#banner ul li.bannerLogo { padding-left: 0px; padding-top: 18px; padding-bottom: 32px;}
#cajaDeBusqInput { height: 17px;   background: url(./public/i/buscadorPlaceholder.png) no-repeat #FFFFFF; background-position: 5px 5px; }
#cajaDeBusqInput:focus  { background: #FFFFFF; }

.search {
  border: 1px solid #ccc;
  font-size: 10px;
  padding: 0.4em;
}

.anchoSearchBanner { width: 425px; }

.btn-search {
    background-image: url("./public/i/lupa.jpg");
	border: none;
    height: 26px;
    text-indent: -999em;
    width: 26px;
	margin-top: 1px;
	margin-left: 4px;
	cursor: pointer;
}

/* fin cabecera  aod*/

/* navegacion principal*/

.botones { background: #76a1b8; border-top: 2px solid #FFFFFF; overflow:hidden; width: 100%; }
.botonesSuperiorLista ul { float:left;position:relative; left:50%; }
.botonesSuperiorLista ul li {float:left; width:161px; height:30px; border-right:2px solid #FFFFFF;  position:relative; right:50%; cursor: pointer; font-size: 12px; font-weight: normal; letter-spacing: 1px;}
.botonesSuperiorLista ul li.first { width:161px; border-left:2px solid #FFFFFF;  }
.botonesSuperiorLista ul li.last { width:161px; }
.botonesSuperiorLista ul li .textoBoton{padding-top: 7px;}


.botonSuperior { background-color: #76a1b8; color: #FFFFFF; }
.botonSuperiorOver { background-color: #3b515c; color: #FFFFFF; }
.botonSuperiorClicked { background-color: #3b515c; color: #76a1b8; }

/* fin navegacion principal*/

/* contenido */
#contenido { width: 100%; }
.huecoInicioPagina {padding-top: 30px; clear: both; }
.tituloVentana { background-color:#76a1b8; padding-bottom: 10px;}
.tituloVentana .hueco{ padding: 15px; }
.separadorVentana {border-top: 1px dashed #999790; width: 100%; margin-top: 10px; margin-bottom: 10px; }
/* fin contenido */


/* pie */
#pie {  width: 100%; background-color: #332f21; float: left; overflow: hidden; text-align: left; color: #999790; font-weight: bold;  }
#pieUL  { float:left; position:relative; left:50%;  width: 980px; padding-top: 30px; padding-bottom: 30px; }
#pieULLI { float: left; position:relative; right:50%; width: 980px; }

.pieInterior { border: 1px solid #999790; background: #332f21;  }

.pieZonaUno { border-bottom: 1px solid #999790; padding-left: 12px; padding-top: 5px; padding-bottom: 5px; }
.pieZonaDos { }
.pieZonaTres { border-top: 1px solid #999790;  height: 58px; }

.pieZonaUno ul {float: left;}

.pieZonaDos ul li { float: left; min-width: 240px;  width: 25%;  }
.pieZonaDos ul li table { border-right: 1px solid #999790; border-collapse: collapse; }
.pieZonaDos ul li table.last { border-right: none; }
.pieZonaDos ul li table tr td { padding-left: 12px; padding-right: 12px; }
.pieZonaDos ul li table tr td.titulo{ padding-top: 5px; padding-bottom: 5px;}
.pieZonaTres ul li {  padding:12px; }
.pieZonaTres ul li.izquierda { float: left; }
.pieZonaTres ul li.derecha { float: right; }

.textoPie {
	line-height:10px;	
	font-size: 8px;
	letter-spacing: 1px;
}

#logoGA_pie { padding: 15px; }

.recuadroPie { border: 1px solid #999790; }
.pieZonaEnlaces { height: 173px; width: 185px; background-color: #FFFFFF; padding: 15px; margin-bottom: 5px; }
.pieZonaEnlaces ul { padding-left: 15px; }
a.pieEnlace { color: #999790 !important; }
a:hover.pieEnlace { color: #666359 !important; }
	
/* fin pie */

/* home contenido */


#divVideo ul { float:left; position:relative; left:50%;  }
#divVideo ul li { float: left; position:relative; right:50%; }

#divVideoImg { z-index:99;  position: relative; top: -115px; }
